<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <nav class="navbar sticky-top navbar-dark navbar-container">
        <div class="container">
            <div class="navbar-header">
                <a href="#"> <img class="nav-logo-kfc" src="~/images/logot.png" /></a>
            </div>
            <a class="nav-a-kfc" href="#">Location</a>
            <a class="nav-a-kfc" href="#">Pinball</a>
            <a class="nav-a-kfc" href="#">Calender</a>
            <a class="nav-a-kfc" href="#">Events</a>
        </div>
    </nav>

    <div class="container-fluid body-content">
        @RenderBody()


    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
